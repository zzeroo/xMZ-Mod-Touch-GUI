<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.16 -->
  <template class="XMZWindow" parent="GtkApplicationWindow">
    <property name="title" translatable="yes">xmz</property>
    <property name="default_width">1024</property>
    <property name="default_height">600</property>
    <property name="icon_name">xmz</property>
    <child>
      <object class="GtkOverlay" id="overlay">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkGrid" id="main_grid">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <child>
              <object class="GtkStack" id="main_stack">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="hexpand">True</property>
                <property name="vexpand">True</property>
                <child>
                  <object class="GtkGrid" id="sensors_list_grid">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <child>

                      <object class="GtkScrolledWindow" id="sensors_scrolled_window">
                        <property name="visible">True</property>
                        <property name="can_focus">True</property>
                        <child>

                          <object class="GtkTreeView" id="sensors_treeview">
                            <property name="visible">True</property>
                            <property name="can_focus">True</property>
                          </object>

                        </child>
                      </object>

                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="left_attach">0</property>
                <property name="top_attach">3</property>
                <property name="width">1</property>
                <property name="height">1</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

